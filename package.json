{
  "name": "api-arc-app",
  "version": "1.0.0",
  "description": "Api for arc app, build with express, typescript, mongoose",
  "scripts": {
    "rm:dist": "rm -rf ./dist",
    "copy:files": "cp .env ./dist/.env",
    "build": "pnpm run rm:dist && cross-env NODE_ENV=production tsc --project tsconfig.build.json && tsc-alias && pnpm run copy:files",
    "dev": "pnpm rm:dist && cross-env NODE_ENV=development npx ts-node-dev --poll --files --respawn --inspect -r tsconfig-paths/register ./src/server.ts",
    "start": "pnpm rm:dist && cross-env NODE_ENV=production ts-node --files --transpile-only -r tsconfig-paths/register ./src/server.ts",
    "lint": "eslint .",
    "lint:src": "eslint src/**/*.ts",
    "lint:fix": "eslint . --fix",
    "test": "vitest --run tests/integration tests/unit",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "pnpm lint:fix",
      "pnpm test"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "5.0.0-beta.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^4.1.0",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "moment": "^2.29.4",
    "mongoose": "6.5.1",
    "node-mocks-http": "^1.13.0",
    "nodemailer": "6.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "validator": "^13.0.0",
    "winston": "3.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@stylistic/eslint-plugin": "^1.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.8.3",
    "@types/nodemailer": "6.4.7",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/ui": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "~8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.1"
  },
  "keywords": [],
  "author": "Hau Tran",
  "license": "MIT"
}
